{
  "name": "gsby",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "watch": "tsc -w",
    "tsc:root-watch": "yarn run tsc:root -w",
    "build": "yarn run tsc:root & tsc",
    "tsc:root": "tsc *.ts --outDir . --esModuleInterop true",
    "gatsby:develop": "gatsby develop",
    "gatsby:build": "gatsby build",
    "develop:ts": "yarn run tsc:root-watch & yarn watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.6",
    "nodemon": "^2.0.6",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "gatsby": "^2.25.2",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-offline": "^3.3.2",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-source-filesystem": "^2.4.1",
    "gatsby-transformer-remark": "^2.9.2",
    "gatsby-transformer-sharp": "^2.5.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  }
}
